/*"Write code for creating a new sorted array from two sorted arrays in O(n+m),
 time complexity where n,m are the size of the unsorted arrays.*/

let arr = [4,2,5,7,1,3,7];
let arr1 = [4,8,1,4,2];
let arr2 = [];

function sortedArray(arr,arr1,arr2)
{
    arr2 = arr.concat(arr1);
    console.log(arr2.sort((a,b)=> a-b));
}

sortedArray(arr,arr1,arr2)
